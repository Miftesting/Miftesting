
//Star Pattern

01)
  
  * * * * *
  public class Pattern {
    public static void main(String[] args) {
        for (int i =  1  ;  i<=5 ; i++)
        {
            System.out.print("* ");
        }
    }
}

  
02)
  
  *
  *
  *
  *
  *
  
  public class Pattern {
    public static void main(String[] args) {
        for (int i =  1  ;  i<=5 ; i++)
        {
            System.out.println("* ");
        }
    }
}


03) 

* * * * *  
* * * * *   
* * * * *   
* * * * *  
* * * * *  
 
public class Pattern {
    public static void main(String[] args) {

        for(int row =  1 ;  row<= 5 ;  row++){
            for (int col =  1  ;  col<=5 ; col++)
            {
                System.out.print("* ");
            }
            System.out.println();

        }

    }
}


04)
  
* 
* *
* * *  
* * * *
* * * * *


 public class Pattern {
        public static void main(String[] args) {
            for (int row = 1 ; row <= 4 ; row++){
                for(int col = 1 ;  col<=row ; col++ ){
                    System.out.print("* ");
                }
                System.out.println();
            }
        }
    }




05)
  
* * * * *
* * * *
* * *
* *
*

    public class Pattern {
        public static void main(String[] args) {
            for (int row = 1 ; row <= 4 ; row++){
                for(int col = 1 ;  col<=5-row ; col++ ){
                    System.out.print("* ");
                }
                System.out.println();
            }
        }
    }


